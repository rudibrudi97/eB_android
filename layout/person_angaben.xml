<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageButton
        android:layout_width="72dp"
        android:layout_height="69dp"
        android:background="@color/grey"
        android:contentDescription="@string/back_to_the_side"
        android:src="@drawable/ic_baseline_arrow_back_ios_new_24_2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="326dp"
        android:layout_height="69dp"
        android:gravity="center"
        android:background="@color/grey"
        android:text="Persönliche Angaben"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textDirection="ltr"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="-0dp" />


    <TextView
        android:layout_width="120dp"
        android:layout_height="30dp"
        tools:ignore="MissingConstraints"
        android:text="Vorname:"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="127dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="Nachname:"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="193dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="E-Mail:"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="303dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="Telefon:"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="421dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="Adresse:"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="485dp" />

    <View
        android:layout_width="350dp"
        android:layout_height="2dp"
        android:background="@color/grey"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="263dp" />

    <View
        android:id="@+id/view4"
        android:layout_width="350dp"
        android:layout_height="2dp"
        android:background="@color/grey"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="378dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:layout_alignParentBottom="true"
        app:menu="@menu/bottom_nav_menu"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="710dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="208dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="mustermann@gmail.com"
        android:textSize="16sp"

        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="295dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="211dp"
        android:layout_height="36dp"
        android:ems="10"
        android:text="0123456789"
        android:textSize="16sp"
        android:inputType="phone"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="421dp" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:text="Max"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="127dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:text="Mustermann"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="193dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine2"
        android:layout_width="174dp"
        android:layout_height="79dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="Musterstraße 1 76133 Karlsruhe"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="474dp" />

</androidx.constraintlayout.widget.ConstraintLayout>